{
	"root": "libs/clients/core",
	"sourceRoot": "libs/clients/core/src",
	"projectType": "library",
	"targets": {
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": [
				"{options.outputFile}"
			],
			"options": {
				"lintFilePatterns": [
					"libs/clients/core/**/*.ts"
				]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": [
				"coverage/libs/clients/core"
			],
			"options": {
				"jestConfig": "libs/clients/core/jest.config.js",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
