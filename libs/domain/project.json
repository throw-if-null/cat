{
  "root": "libs/domain",
  "sourceRoot": "libs/domain/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/domain",
        "main": "libs/domain/src/index.ts",
        "tsConfig": "libs/domain/tsconfig.lib.json",
        "assets": [
          "libs/domain/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/domain/**/*.ts"
        ]
      }
    }
  },
  "tags": [
    "scope:shared",
    "type:util"
  ]
}
