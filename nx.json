{
	"npmScope": "cat",
	"affected": {
		"defaultBase": "master"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/nx-cloud",
			"options": {
				"cacheableOperations": [
					"build",
					"lint",
					"test",
					"e2e"
				],
				"accessToken": "MGNmYTVjMjQtMDA1OS00NmMxLTg4MzYtNjMyOTkyNzVmNzUyfHJlYWQtd3JpdGU=",
				"canTrackAnalytics": false,
				"showUsageWarnings": true,
				"parallel": 1
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	},
	"cli": {
		"defaultCollection": "@nrwl/angular"
	},
	"generators": {
		"@nrwl/angular": {
			"application": {
				"linter": "eslint"
			},
			"library": {
				"linter": "eslint"
			},
			"storybook-configuration": {
				"linter": "eslint"
			}
		},
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"style": "scss"
		},
		"@nrwl/js:library": {
			"buildable": true
		}
	},
	"defaultProject": "cat"
}
