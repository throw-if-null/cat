    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "migrate": "nx migrate latest",
    "migrate:run": "nx migrate --run-migrations",
    "g:comp": "nx generate @nrwl/angular:component --project=noda",
    "generate:build-info": "nx workspace-generator build-information cat",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down --remove-orphans",
    "docker:build": "docker build -t cat .",
    "docker:cat:run": "docker run -p 4200:80 cat:latest",
    "build:cli": "nx build client-cli",
    "run:cli": "node --experimental-fetch dist/libs/clients/cli/src/index.js"
