    "start": "ng serve",
    "watch:hmr": "ng serve --hmr",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "test": "ng test",
    "test:all": "nx run-many --target=test --all --codeCoverage --coverageReporters=lcov",
    "coverage": "ng test --coverage",
    "bundle-report": "webpack-bundle-analyzer dist/apps/cat/stats.json",
    "lint": "nx workspace-lint && ng lint",
    "readycheck": "npm run lint && npm run test && npm run build",
    "e2e": "nx e2e cat-e2e",
    "cy:open": "cd apps/cat-e2e cypress open",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
